C[0-9][0-9]* : force
	@ echo "start $@ $(MODE)"
	@ cd $@ ; $(MAKE) -f ../Makefile.test $(MODE) --no-print-directory > .message 2>&1; cd ..
	@ echo "finished $@ $(MODE)"; cat $@/.message

force: ;

